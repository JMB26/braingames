<form method="post" action="{{ path('app_users_delete', {'id': user.id}) }}" onsubmit="return confirm('Etes-vous certain de vouloir supprimer cet utilisateur ?');">
    <input type="hidden" name="_token" value="{{ csrf_token('delete' ~ user.id) }}">
    <button class="btn">SUPPRIMER</button>
</form>
